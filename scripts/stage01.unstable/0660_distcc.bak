		<unpack>
			<![CDATA[ 
			tar xzf ${SRCDIR}/${PKGNAME}-${PKGVERSION}.tar.gz
			]]>
		</unpack>
		<build>
			<![CDATA[ 
			cd ${PKGNAME}
			bash autogen.sh
			./configure --prefix=/tools --disable-Werror --without-avahi 
			make
			]]>
		</build>
		<install>
			<![CDATA[
			cd ${PKGNAME}
			make install
			]]>
		</install>
