<llpackages>
    <package name='doxygen' version='1.8.11' install='chroot'>
        <license>
            unknown
        </license>
        <sources>
            <file>
                <pkg sha1='c0841007cf8a7910ccf0d8e4f7a5dd8fd2d9fc18'>
                    doxygen-1.8.11.src.tar.gz
                </pkg>
                <mirror>
                    http://ftp.stack.nl/pub/users/dimitri/
                </mirror>
            </file>
            <manualcheck date='20160412' interval='60' mirror='http://ftp.stack.nl/pub/users/dimitri/'/>
        </sources>
	<builddeps>
                        <dep>M2Crypto</dep>
                        <dep>Python</dep>
                        <dep>WebOb</dep>
                        <dep>bash</dep>
                        <dep>binutils</dep>
                        <dep>bison</dep>
                        <dep>cmake</dep>
                        <dep>coreutils</dep>
                        <dep>diffutils</dep>
                        <dep>flex</dep>
                        <dep>fusepy</dep>
                        <dep>gcc</dep>
                        <dep>glibc</dep>
                        <dep>libxml2</dep>
                        <dep>linux_headers</dep>
                        <dep>m4</dep>
                        <dep>make</dep>
                        <dep>openssl</dep>
                        <dep>plaso</dep>
                        <dep>pygobject</dep>
                        <dep>python-dateutil</dep>
                        <dep>setuptools</dep>
                </builddeps>
        <unpack>
            <![CDATA[tar xf ${SRCDIR}/doxygen-${PKGVERSION}.src.tar.gz
]]>
        </unpack>
        <patch/>
        <build>
            <![CDATA[cd doxygen-${PKGVERSION}/
	    mkdir build 
	    cd build 
	    cmake -G "Unix Makefiles" -DCMAKE_INSTALL_PREFIX=/usr  ..
	    make
]]>
        </build>
        <install>
            <![CDATA[cd doxygen-${PKGVERSION}/build
	    make install
]]>
        </install>
        <clean>
            <![CDATA[rm -rf doxygen-${PKGVERSION}
]]>
        </clean>
    </package>
</llpackages>